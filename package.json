{
    "private": true,
    "scripts": {
        "start": "concurrently 'npm run serve' 'npm run tailwind:process --watch'",
        "build": "eleventy && npm run tailwind:process",
        "serve": "eleventy --serve",
        "tailwind:process": "npx postcss src/styles/global.css --o _site/styles/global.css"
    },
    "devDependencies": {
        "@11ty/eleventy": "^1.0.1",
        "@11ty/eleventy-plugin-syntaxhighlight": "^4.1.0",
        "@tailwindcss/typography": "^0.5.4",
        "autoprefixer": "^10.4.8",
        "concurrently": "^7.3.0",
        "date-fns": "^2.29.1",
        "postcss-cli": "^10.0.0",
        "tailwindcss": "^3.1.7"
    }
}
